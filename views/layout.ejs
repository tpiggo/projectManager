<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="Project Manager">
        <title>Project Manager</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="/css/index.css">
    </head>
    <body>
        <!-- Navbar -->
        <nav id="main-nav">
            <div id='left-nav'>
                <a class="nav-item" href="/">Home</a>
                <% for(let element of renderable) { %>
                    <div class="nav-item" type="priority" onclick="clickableElement(event, this,  <%= element.id %> )" ><%= element.name %> Priorities</div>
                <% } %> 
                <div class="nav-item">Strategic Enablers</div>
                <div class="nav-item">
                    <!-- Search Box -->
                    Searchable Box
                </div>
            </div>
            <a class="nav-item nav-link dropdown-toggle" href="#" id="UserMenu" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-person-circle" viewBox="0 0 16 16">
                    <path d="M13.468 12.37C12.758 11.226 11.195 10 8 10s-4.757 1.225-5.468 2.37A6.987 6.987 0 0 0 8 15a6.987 6.987 0 0 0 5.468-2.63z"/>
                    <path fill-rule="evenodd" d="M8 9a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
                    <path fill-rule="evenodd" d="M8 1a7 7 0 1 0 0 14A7 7 0 0 0 8 1zM0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8z"/>
                    </svg>
            </a>
            <div id="user-info" class="dropdown-menu" aria-labelledby="UserMenu" role="menu">
                <li><a class="dropdown-item" id="login" href="/login">Login</a></li>
            </div>
        </nav>
        <%- body %> 
    </body>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>
    <% for(let script of scripts) { %>
        <script type='text/javascript' src=<%= script %>></script>
    <% } %> 
</html>